{% if gva_theme_setting('map_source') == 'google' or gva_theme_setting('map_source') == '' %}
  {{ attach_library('ziston/listing_init.gmap') }}
{% endif %} 
{{ attach_library('ziston/listing_init') }}

{% set has_breadcrumb = '' %}
<div class="gva-body-wrapper">
	<div class="body-page gva-body-page">
	   {% include header_skin %}
		
		{% if page.breadcrumbs %}
			{% set has_breadcrumb = ' has-breadcrumb' %}
			<div class="breadcrumbs hidden">
				{{ page.breadcrumbs }}
			</div>
		{% endif %}

		<div role="main" class="main main-page{{ has_breadcrumb }}">
			
			{% if page.help %}
				<div class="help gav-help-region">
					<div class="container">
						<div class="row">
							<div class="content-inner col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
								{{ page.help }}
							</div>
						</div>
					</div>
				</div>
			{% endif %}
			
			<div class="clearfix"></div>
			{% if page.before_content %}
				<div class="before_content area">
					<div class="container">
						<div class="row">
							<div class="col-xs-12">
								{{ page.before_content }}
							</div>
						</div>
					</div>
				</div>
			{% endif %}
			
			<div class="clearfix"></div>
			
			<div id="content" class="content content-full">
				<div class="container-full container-bg">
					{% include '@ziston/page/main-no-sidebar.html.twig' %}
				</div>
			</div>

			{% if page.highlighted %}
				<div class="highlighted area">
					<div class="container">
						{{ page.highlighted }}
					</div>
				</div>
			{% endif %}

			{% if page.after_content %}
				<div class="area after-content">
					<div class="container">
		          	<div class="content-inner">
							 {{ page.after_content }}
		          	</div>
	        		</div>
				</div>
			{% endif %}
			
		</div>

	</div>

	{% include '@ziston/page/footer.html.twig' %}
</div>

