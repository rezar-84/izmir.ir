{% if gva_theme_setting('map_source') == 'google' or gva_theme_setting('map_source') == '' %}
  {{ attach_library('ziston/listing_init.gmap') }}
{% endif %} 
{{ attach_library('ziston/listing_init') }}

<div class="gva-body-wrapper gva-listings-map-page listing-taxonomy-page mode-filter-fixed">
	<div class="body-page gva-body-page">
	   {% include header_skin %}
		
		{% if page.breadcrumbs %}
			<div class="breadcrumbs">
				{{ page.breadcrumbs }}
			</div>
		{% endif %}

		<div role="main" class="main main-page">

			{% if page.help %}
				<div class="help gav-help-region">
					<div class="container">
						<div class="row">
							<div class="content-inner col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
								{{ page.help }}
							</div>
						</div>
					</div>
				</div>
			{% endif %}
			
			<div class="clearfix"></div>
			
			<div id="content" class="content content-full">
				<div class="container-full">
					{% if page.content %}
						<div class="content-main">
							{{ page.content }}
						</div>
					{% endif %}
				</div>
			</div>

		</div>
	</div>
	{% include '@ziston/page/footer.html.twig' %}
</div>

